{
 "parser": "babel-eslint",
 "parserOptions": {
  "ecmaVersion": 6,
  "sourceType": "module"
 },
 "plugins": [
  "babel"
 ],
 "extends": "airbnb/base",
 "env": {
  "browser": true,
  "jquery": true,
  "es6": true
 },
 "globals": {
  "Promise": true
 },
 "rules": {
  "linebreak-style": [
   "error",
   "windows"
  ],
  "comma-dangle": [
   "error",
   "only-multiline"
  ],
  "padded-blocks": [
   "error",
   "always"
  ]
 }
}